<AnchorPane id="AnchorPane" prefHeight="363.0" prefWidth="517.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.unideb.inf.skipmaster.Controller.LoginScreenController">
