            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="loginneptun" layoutX="184.0" layoutY="150.0" />
      <ImageView fitHeight="114.0" fitWidth="185.0" layoutX="172.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
